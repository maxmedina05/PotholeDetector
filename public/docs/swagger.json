{
  "swagger": "2.0",
  "info": {
    "description": "This is the documentation for the Pothole Detector api.",
    "version": "0.1.0",
    "title": "Pothole Dector API",
    "contact": {
      "email": "maxmedina05@gmail.com"
    },
    "license": {
      "name": "MIT",
      "url": "https://github.com/swagger-api/swagger-ui/blob/master/LICENSE"
    }
  },
  "host": "localhost:5099",
  "basePath": "/",
  "produces": [
    "application/json"
  ],
  "consumes": [
    "application/json"
  ],
  "schemes": [
    "http"
  ],
  "paths": {
    "/users": {
      "x-swagger-router-controller": "user.controller",
      "get": {
        "description": "Returns a List of users to the caller",
        "operationId": "getUsers",
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/UsersResponse"
            }
          },
          "default": {
            "description": "Error",
            "schema": {
              "$ref": "#/definitions/ErrorResponse"
            }
          }
        }
      },
      "post": {
        "description": "Creates a User",
        "operationId": "addUser",
        "parameters": [
          {
            "name": "firstName",
            "in": "body",
            "description": "The first name of the person that will be created",
            "required": false,
            "type": "string"
          },
          {
            "name": "lastName",
            "in": "body",
            "description": "The last name of the person that will be created",
            "required": false,
            "type": "string"
          },
          {
            "name": "email",
            "in": "body",
            "description": "The email of the person that will be created",
            "required": false,
            "type": "string"
          },
          {
            "name": "password",
            "in": "body",
            "description": "The password of the person that will be created",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/UsersResponse"
            }
          },
          "default": {
            "description": "Error",
            "schema": {
              "$ref": "#/definitions/ErrorResponse"
            }
          }
        }
      }
    },
    "/swagger": {
      "x-swagger-pipe": "swagger_raw"
    }
  },
  "definitions": {
    "UsersResponse": {
      "required": [
        "success",
        "message"
      ],
      "properties": {
        "success": {
          "type": "boolean"
        },
        "message": {
          "type": "string"
        },
        "data": {
          "type": "object"
        }
      }
    },
    "ErrorResponse": {
      "required": [
        "message"
      ],
      "properties": {
        "message": {
          "type": "string"
        }
      }
    }
  }
}
